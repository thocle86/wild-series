<form method="post" action="{{ path('season_delete', {'idSeason': season.id}) }}" onsubmit="return confirm('Etes-vous certain de vouloir supprimer cette saison?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ season.id) }}">
    <button class="submit">Supprimer</button>
</form>
